<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Williams Trading System{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% block stylesheets %} 
            {% stylesheets filter="cssrewrite,scssphp" output='css/compiled/login.css'
                'bower_components/bootstrap/dist/css/bootstrap.css'
                'bower_components/metisMenu/dist/metisMenu.css'
                'bower_components/font-awesome/css/font-awesome.css'
                'bower_components/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        {% block javascripts %}            
            {% javascripts filter="?jsqueeze" output='js/compiled/login.js'
                'bower_components/jquery/dist/jquery.js'
                'bower_components/bootstrap/dist/js/bootstrap.js'
                'bower_components/metisMenu/dist/metisMenu.js'
                'bower_components/startbootstrap-sb-admin-2/dist/js/sb-admin-2.js'
            %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </head>
    <body>     

        {% block fos_user_content %}{% endblock %}

    </body>
</html>
